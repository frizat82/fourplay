<MudDialog>
    <DialogContent>
            Team: @TeamAbbr  <MudImage Src="@Logo" Width="50" />
            <MudList T="string">
        @foreach (var user in @UserNames)
        {
                <MudListItem Value="@user" Icon="@Icons.Material.Filled.Inbox">
                                <AvatarContent>
                                    <MudAvatar Color="Color.Tertiary">@user.First()</MudAvatar>
                                </AvatarContent>
                                <ChildContent>
                                    @user
                                </ChildContent>
                            </MudListItem>
        }
            </MudList>
    </DialogContent>
</MudDialog>